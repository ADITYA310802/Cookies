<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Cookies1</display-name>

	<!-- Servlet 1 -->
	<servlet>
		<servlet-name>ck1</servlet-name>
		<servlet-class>com.cook.Cookie1</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ck1</servlet-name>
		<url-pattern>/cook1</url-pattern>
	</servlet-mapping>

	<!-- Servlet 2 -->

	<servlet>
		<servlet-name>ck2</servlet-name>
		<servlet-class>com.cook.Cookie2</servlet-class>
		
	</servlet>

	<servlet-mapping>
		<servlet-name>ck2</servlet-name>
		<url-pattern>/cook2</url-pattern>
	</servlet-mapping>
</web-app>